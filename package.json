{
  "name": "dev-jot",
  "version": "1.0.0",
  "description": "note-taking app for online developer tutorials",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write ."
  },
  "author": "James Robinson <jrobind@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jrobind/dev-jot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrobind/dev-jot.git"
  },
  "keywords": [
    "notes",
    "dev",
    "online",
    "lessons"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run prettier"
    }
  },
  "devDependencies": {
    "husky": "^4.3.6",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  }
}
